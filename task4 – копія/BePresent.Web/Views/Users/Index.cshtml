@model IEnumerable<BePresent.Domain.Users.User>

<h1>Users</h1>

<table>
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
            <th>Date of Birth</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.Email</td>
                <td>@user.DateOfBirth</td>
                <td>
                    <!-- Додайте кнопки для редагування/видалення -->
                </td>
            </tr>
        }
    </tbody>
</table>
